{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:mobile": "playwright test --project='Mobile iPhone SE'",
    "test:e2e:desktop": "playwright test --project='Desktop Chrome'",
    "test:e2e:demo": "playwright test tests/e2e/demo-mode-mobile.spec.ts",
    "test:e2e:demo:mobile": "playwright test tests/e2e/demo-mode-mobile.spec.ts --project='Mobile iPhone SE'",
    "test:e2e:chef": "playwright test tests/e2e/chef-virtual.spec.ts",
    "test:e2e:chef:ui": "playwright test tests/e2e/chef-virtual.spec.ts --ui",
    "test:e2e:chef:mobile": "playwright test tests/e2e/chef-virtual.spec.ts --project='Mobile iPhone SE'",
    "test:e2e:chef:desktop": "playwright test tests/e2e/chef-virtual.spec.ts --project='Desktop Chrome'",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
